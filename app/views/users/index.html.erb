<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Email</th>
      <th>Entity</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.code %></td>
        <td><%= user.email %></td>
        <td><%= user.entity %></td>
        <td><%= user.report_type %></td>
        <td><%= link_to 'Show', user %></td>
        <td>|</td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table><br>

<%= link_to 'New User', new_user_path %>
